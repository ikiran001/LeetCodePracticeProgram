package LeetCodeArrays;

public class LeetCode198 {

	public static void main(String[] args) {


	}

	public int rob(int[] nums) {
		int odd=0;
		int even=0;
		for (int i = 0; i < nums.length/2; i++) {
			odd+=nums[i*2];
		}
		for (int i = 0; i < nums.length/2; i++) {
			even+=nums[i*2+1];
		}
	}
}
