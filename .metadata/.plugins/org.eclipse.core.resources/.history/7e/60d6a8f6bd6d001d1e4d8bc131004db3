package Practice;

public class LeetCode258 {

	public static void main(String[] args) {


	}
	
static int addDigits(int num) {
        while(num>0) {
        	int sum=0;
        	while(num>0) {
        		int modulo=num%10;
        		sum+=modulo;
        		num=num/10;
        	}
        	num=sum;
        }
        return num;
    }

}
