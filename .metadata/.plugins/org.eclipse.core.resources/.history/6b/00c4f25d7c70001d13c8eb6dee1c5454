package Stringssss;

public class LeetCode168 {

	public static void main(String[] args) {

int a=26;
System.out.println(convertToTitle(a));
	}
	
	 static String convertToTitle(int columnNumber) {
	        String series="";
	        int i=0;
	        while(columnNumber>=i){
	        	char ch=(char)('A'+i);
	        	series+=ch;
	        
	        	i++;
	        	if(columnNumber==i) {
	        		return  String.valueOf(series.charAt(i-1));
	        	}
	        }
	        return "";
	    }
	 
	 
	 static String aTOa(int columnNumber) {

	        String series="";
	        int i=0;
	        while(columnNumber>=i){
	        	char ch=(char)('A'+i);
	        	series+=ch;
	        
	        	i++;
	        	if(columnNumber==i) {
	        		return  String.valueOf(series.charAt(i-1));
	        	}
	        }
	        return "";
	    
	 }

}
