package GeeksforGeeks;

public class GGElementOccurNtimes {

	public static void main(String[] args) {
		int N = 7; int K = 2;
				int A[] = {1, 7, 4, 3, 4, 8, 7};
	}
	  public static int firstElementKTime(int[] a, int n, int k) { 
		  int max=0;
		  
		    for (int i = 0; i < a.length-1; i++) {
				for (int j = i+1; j < a.length; j++) {
					if(a[i]==a[j]) {
						int c=j;
						if(c>max) {
							max=c;
						}
						return c;
						
					}
				}
			}
		    return -1;
	        
	    } 

}
