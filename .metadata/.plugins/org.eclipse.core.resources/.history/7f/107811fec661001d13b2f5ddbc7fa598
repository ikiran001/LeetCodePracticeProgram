package HackerRank;

public class FizzBuss {

	public static void main(String[] args) {
		int n =20;
		fizzBuzz(n);

	}
	public static void fizzBuzz(int n) {
		String a =String.format("%d", )
		String b="Buzz";
		String c="FizzBuzz";
		for (int i = 1; i < n; i++) {
			if (i%3==0) {
				i=String.format("Fizz", i);
			}
			System.out.println(i);
		}
	}

}
