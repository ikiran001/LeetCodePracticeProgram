package HackerRank;

public class FizzBuss {

	public static void main(String[] args) {
		int n =20;
		fizzBuzz(n);

	}
	public static void fizzBuzz(int n) {
		String a ="Fizz";
		String b="Buzz";
		String c="FizzBuzz";
		for (int i = 0; i < n; i++) {
			if (i%3==0 && i%5==0) {
				i=50;

			}
			else if (i%3==0) {
				i=30;
			}
			else if(i%5==0) {
				i=55;
			}

			System.out.println(i);
		}
	}

}
