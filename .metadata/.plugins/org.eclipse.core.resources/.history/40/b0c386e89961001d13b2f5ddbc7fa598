package LeetCodeArrays;

public class LeetCode922 {

	public static void main(String[] args) {


	}
	 public int[] sortArrayByParityII(int[] nums) {
		 int[] result=new int[nums.length];
	        for (int i = 0; i < nums.length; i++) {
				if(nums[i]%2==0) {
					result[i*2]=nums[i];
				}
				else {
					result[i*2+1]=nums[i];
				}
			}
	        return result;
	    }

}
