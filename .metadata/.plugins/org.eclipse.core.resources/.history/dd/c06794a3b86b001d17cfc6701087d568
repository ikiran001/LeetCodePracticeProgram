package LeetCodeSort;

import java.util.Arrays;

public class LeetCode645 {

	public static void main(String[] args) {

	}
	public static int[] findErrorNums(int[] nums) {
		Arrays.sort(nums);
		for(int i=1;i<nums.length;i++){
			if(nums[i]==nums[i-1]){
				nums[i]=nums[i-1]+1;
				return new int[]{nums[i-1],nums[i]};
			}
		}
		return nums;
	}
}
