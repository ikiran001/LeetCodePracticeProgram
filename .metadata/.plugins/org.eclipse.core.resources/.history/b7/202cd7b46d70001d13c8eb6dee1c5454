package Practice;
//9. Palindrome Number
//https://leetcode.com/problems/palindrome-number/
public class LeetCode9 {

	public static void main(String[] args) {
		int x = 121;

	}
	 static boolean isPalindrome(int x) {
	      String a=String.valueOf(x);
	      String [] arr=a.split("");
	      int i=0;
	      int j=arr.length-1;
	      while(i<=j) {
	    	  if(i==j) {
	    	  i++;
	    	  j--;
	    	  }
	    	  else return false;
	      }
	        return true;
	    }

}
