package LeetCodeSort;

public class LeetCode350 {

	public static void main(String[] args) {


	}
	 public int[] intersect(int[] nums1, int[] nums2) {
	        int index=0;
	        int[] arr=new int[2];
	        for (int i = 0; i < nums1.length; i++) {
				for (int j = 0; j < nums2.length; j++) {
					if(nums1[i]==nums2[j]) {
						arr[index]=nums1[i];
						index++;
					}
				}
			}
	        return arr;
	    }

}
