package Stringssss;
//issue
public class LeetCode168 {

	public static void main(String[] args) {

		int a=26;
		System.out.println(convertToTitle(a));
	}

	static String convertToTitle(int columnNumber) {
		String series="";
		for (int i = 0; i < 26; i++) {
			char ch=(char)('A'+i);
			for (int j = 0; j < 26; j++) {
				series+=ch;
			}
		}


		return String.valueOf(series.charAt(columnNumber));
	}



}


