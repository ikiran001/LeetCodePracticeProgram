package LeetCodeArrays;

public class LeetCode55 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	public boolean canJump(int[] nums) {
		int i=0;
		while(i<=nums.length-1) {
			i=nums[i];
			if(nums[i]==nums[nums.length-1]) {
				return true;
			}
		}
		return false;
	}
}
