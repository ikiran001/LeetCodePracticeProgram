package MathsForDSA;

import java.util.ArrayList;

public class LeetCode1492 {

	public static void main(String[] args) {
		int  n = 4; int k = 4;
		System.out.println(kthFactor(n, k));

	}
	public static int kthFactor(int n, int k) {
		ArrayList<Integer> list= new ArrayList<>();
		for(int i=1; i<=n;i++){
			if(n%i==0){
				list.add(i);
			}
		}
		return list.get(k-1);
	}

}
