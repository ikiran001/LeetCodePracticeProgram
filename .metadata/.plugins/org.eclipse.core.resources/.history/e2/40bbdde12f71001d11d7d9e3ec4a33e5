package Stringssss;

public class LeetCode67 {

	public static void main(String[] args) {
		String a = "1010000010010011011001000001010111101101100110111011111111110100000010111100111000111110000110";
		String b = "11010100101110111000111110011000101010000110101110101000001101101100101110111100110000001101111001";

		System.out.println(addBinary(a, b));
	}
	static String addBinary(String a, String b) {

		Long x=(long) Integer.parseInt(a, 2);

		Long y=(long) Integer.parseInt(b, 2);

		Long sum=x+y;

		Long.toBinaryString(sum);

		return Long.toBinaryString(sum);


	}

}	
