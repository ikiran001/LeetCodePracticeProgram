package LeetCodeSort;

public class LeetCode287 {

	public static void main(String[] args) {
		int[] arr= {3,1,3,4,2};
System.out.println(findDuplicate(arr));
	}
	static int findDuplicate(int[] nums) {
		
		for (int i = 0; i < nums.length; i++) {
			int count=0;
			for(int num:nums) {
				if(num==nums[i]) {
					count++;
					if(count==2) {
					return nums[i];}
				}
			}
		}
		return -1;    
	}
}
