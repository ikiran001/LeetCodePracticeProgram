package LeetCodeArrays;

public class LeetCode1351 {

	public static void main(String[] args) {

int [][] arr= {
		{4,3,2,-1},
		{3,2,1,-1},
		{1,1,-1,-2},
		{-1,-1,-2,-3}
};
	}

	
public int countNegatives(int[][] grid) {
	int count=0;
        for (int i = 0; i < grid.length; i++) {
			for (int j = 0; j < grid.length; j++) {
				if (grid[i][j]<0) {
					
					count++;
				}
			}
		}
    }
}
