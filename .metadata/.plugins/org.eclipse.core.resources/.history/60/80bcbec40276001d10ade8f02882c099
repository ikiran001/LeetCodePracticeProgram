package MathsForDSA;

public class LeetCode1492 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	public int kthFactor(int n, int k) {
		ArrayList<Integer> list= new ArrayList<>();
		for(int i=1; i<=n;i++){
			if(n%i==0){
				list.add(i);
			}
		}
		return list.get(k-1);
	}

}
