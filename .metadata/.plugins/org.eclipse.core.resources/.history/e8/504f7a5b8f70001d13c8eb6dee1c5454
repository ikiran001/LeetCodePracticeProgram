package Stringssss;
//issue
public class LeetCode50 {

	public static void main(String[] args) {

		double x=3; int n=3;
		System.out.println(myPow(x, n));
	
	}

	  static double myPow(double x, int n) {
		  double a=x;
	        for (int i = 1; i < n; i++) {
				x*=a;
			}
	        return x;
	    }

}


