package HackerRank;

public class FizzBuss {

	public static void main(String[] args) {
		int n =20;
		fizzBuzz(n);

	}
	public static void fizzBuzz(int n) {
		String a ="Fizz";
		String b="Buzz";
		String c="FizzBuzz";
		for (int i = 1; i < n; ) {
			if (i%3==0) {
				i=Integer.parseInt("Fizz");
				i++;
			}
			System.out.println(i);
		}
	}

}
