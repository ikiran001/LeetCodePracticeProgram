package HackerRank;

public class FizzBuss {

	public static void main(String[] args) {


	}
	public static void fizzBuzz(int n) {
String a ="Fizz";
String b="Buzz";
String c="FizzBuzz";
		for (int i = 0; i < n; i++) {
			if (i%3==0 && i%5==0) {
				i=Integer.parseInt(c);
			}
			else if (i%3==0) {
				i=Integer.parseInt(a);
			}
			else if(i%5==0) {
				i=Integer.parseInt(b);
			}
			
			System.out.println(i);
		}
	}

}
