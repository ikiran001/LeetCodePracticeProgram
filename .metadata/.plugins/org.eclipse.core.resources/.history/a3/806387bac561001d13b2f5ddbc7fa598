package HackerRank;

public class FizzBuss {

	public static void main(String[] args) {
		int n =20;
		fizzBuzz(n);

	}
	public static void fizzBuzz(int n) {
		String a ="Fizz";
		String b="Buzz";
		String c="FizzBuzz";
		for (int i:n) {
			if (i%3==0 && i%5==0) {
				i=50;
				i++;
			}
			else if (i%3==0) {
				i=30;
				i++;
			}
			else if(i%5==0) {
				i=55;i++;
			}


		}

	}

}
