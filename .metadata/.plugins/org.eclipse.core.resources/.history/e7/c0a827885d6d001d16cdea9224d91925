package SortingAlgos;
//best sortign algo for 1-n
public class CyclicSort {

	public static void main(String[] args) {
		

	}
static void cyclicSort(int [] nums) {
	int i=0;
	while(i<nums.length) {
		int correct=nums[i]-1;
		if(nums[i]!=nums[correct]) {
			swap(nums,i,correct);
			
		}
		else {
			i++;
		}
	}
}
}
