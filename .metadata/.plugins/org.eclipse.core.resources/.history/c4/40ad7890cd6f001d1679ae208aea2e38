package Stringssss;

public class LeetCode680 {

	public static void main(String[] args) {

	}
	 public boolean validPalindrome(String s) {
		 int count=0;
	        for (int i = 0; i < s.length(); i++) {
				int start=s.charAt(i);
				int end=s.charAt(s.length())-i-1;
				if(start!=end) {
					end--;
				}
			}
	        return true;
	    }
}
