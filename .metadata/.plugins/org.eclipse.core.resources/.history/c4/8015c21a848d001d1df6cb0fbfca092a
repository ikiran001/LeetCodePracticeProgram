	}
	   public int strStr(String haystack, String needle) {
		   
		  
		   if(haystack.length()==0) {
			   return -1;
		   }
		   
		if(   haystack.substring(i, needle.length()).contains(needle)) {
			return i;
		}
		else {
			return helper(haystack, needle, 0);
			
		}
		   
	   }
	   
	   public int helper(String stack,String needle, int i) {
		   if(stack.contains(needle)) {
			   
		   }
		   return helper(stack.substring(i, needle.length()+1), needle, i+1);
	   }

}
