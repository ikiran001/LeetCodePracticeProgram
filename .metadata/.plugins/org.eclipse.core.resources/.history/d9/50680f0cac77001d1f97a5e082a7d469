package Recursion;

public class LeetCode7 {

	public static void main(String[] args) {
		int a=123;
		System.out.println(reverse(a));

	}
	
	  public static int reverse(int x) {
	        if(x%10==x) {
	        	return x;
	        }
	        int mod=x%10;
	        return (mod*10)+reverse(x/10);
	        		
	    }

}
