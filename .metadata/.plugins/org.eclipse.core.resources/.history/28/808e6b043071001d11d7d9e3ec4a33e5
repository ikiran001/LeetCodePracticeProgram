package Stringssss;

public class LeetCode67 {

	public static void main(String[] args) {
		String a = "101000001001001101100100000101011110110110011011101111111111010000001011110011100011111000011";
		String b = "110101001011101110001111100110001010100001101011101010000011011011001011101111001100000011011110011";

		System.out.println(addBinary(a, b));
	}
	static String addBinary(String a, String b) {
int length=a.length();
System.out.println(length);
		Long x=(long) Integer.parseInt(a, 2);

		Long y=(long) Integer.parseInt(b, 2);

		Long sum=x+y;

		Long.toBinaryString(sum);

		return Long.toBinaryString(sum);


	}

}	
