	public String decodeMessage(String key, String message) {
		String alphabets="";
		String ans="";
		for(int a=0 ;a<26 ;a++) {
			char ch=(char)(a+'a');
			alphabets+=ch;
		}
		Map<Character, Character> map=new HashMap<>();
		for (int i = 0; i < 26; i++) {
			map.put(alphabets.charAt(i), key.charAt(i));
		}
		for(char cha:message.toCharArray()) {
			
		}
		



	}
}
