package Practice;

public class LeetCode202 {

	public static void main(String[] args) {
int a=2;
System.out.println(isHappy(a));

	}
	
	static boolean isHappy(int n) {	
    while(n!=1) {
    	int sum=0;
    	while(n>0) {
    		
    		int m= n%10;
    		sum+= m*m;
    		n=n/10;
    	}
    	n=sum;
    } 
    return true;
    }

}
