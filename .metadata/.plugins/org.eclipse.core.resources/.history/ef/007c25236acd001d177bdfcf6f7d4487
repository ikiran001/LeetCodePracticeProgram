package LeetCodeArrays;

public class LeetCode1909 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static boolean oneEle(int[] nums) {
		int c=0;
		for (int i = 0; i < nums.length; i++) {
			if(nums[i]<=nums[i+1]) {
				c++;
			}
			if(c==2) {
				return false;
			}
		} 
		return true;
	}

}
